            "executable": "PolyControl/build/PolyControl.elf",
            "name": "Debug PolyControl",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "preLaunchTask": "Build PolyControl",
            "device": "stlink",
